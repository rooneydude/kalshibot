# Kalshi API (generate at https://demo.kalshi.co/ -> Account & Security -> API Keys)
KALSHI_API_KEY_ID=your-api-key-uuid-here
KALSHI_RSA_PRIVATE_KEY="-----BEGIN RSA PRIVATE KEY-----\nYOUR_KEY_HERE\n-----END RSA PRIVATE KEY-----"
KALSHI_BASE_URL=https://demo-api.kalshi.co/trade-api/v2

# Anthropic
ANTHROPIC_API_KEY=sk-ant-...

# Database (auto-provided by Railway Postgres addon)
DATABASE_URL=postgresql://user:password@host:5432/dbname

# Discord (create a webhook in your Discord server channel settings)
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/...

# FRED (optional, free key from https://fred.stlouisfed.org/)
FRED_API_KEY=your-fred-api-key
